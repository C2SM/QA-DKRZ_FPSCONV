netcdf cf_5.4 {
dimensions:
   station = 2 ; // measurement locations
   pressure = 3 ; // pressure levels
   time = UNLIMITED ;
   name_strlen = 20 ;

variables:
   double time(time) ;
      time:standard_name = "time";
 	    time:long_name = "time of measurement" ;
 	    time:units = "days since 1970-01-01 00:00:00" ;
   float lon(station) ;
 	    lon:long_name = "station longitude";
 	    lon:units = "degrees_east";
   float lat(station) ;
 	    lat:long_name = "station latitude" ;
 	    lat:units = "degrees_north" ;
   float humidity(time,pressure,station) ;
      humidity:long_name = "specific_humidity" ;
      humidity:coordinates = "lat lon" ;

   float pressure(pressure) ;
 	    pressure:units = "hPa" ;
      pressure:comment = "standard_name attribute is strongly recommended for coordinate variables";

// global attributes:
   :comment="Timeseries of station data" ;
   :Conventions = "CF-1.4" ;

data:
time = 1., 2.;
pressure = 1., 2., 3.;
}
