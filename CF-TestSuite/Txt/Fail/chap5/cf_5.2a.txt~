netcdf cf_5.2a {
dimensions:
   xc = 2 ;
   yc = 3 ;
   lev = 4 ;

variables:
   float xc(xc) ;
	     xc:long_name = "x-coordinate in Cartesian system" ;
	     xc:units = "m" ;
   float yc(yc) ;
	     yc:long_name = "y-coordinate in Cartesian system" ;
	     yc:units = "m" ;
   float lev(lev) ;
	     lev:long_name = "pressure level" ;
	     lev:units = "hPa" ;
	  float lon(yc,xc) ;
      lon:axis = "X" ;
	     lon:long_name = "longitude" ;
	     lon:units = "degrees_east" ;
	  float lat(yc,xc) ;
      lat:axis = "X" ;
	     lat:long_name = "latitude" ;
	     lat:units = "degrees_north" ;

   float T(lev,yc,xc) ;
      T:long_name = "temperature" ;
      T:units = "K" ;
      T:coordinates = "lat lon xc yc" ;
      T:comment= "invalid: faulty lat=axis, should be Y" ;

// global attributes:
   :Conventions = "CF-1.6" ;

data:
yc = 1., 2.;
xc = 1., 2., 3.;
lev = 1., 2., 3., 4;
}