netcdf cf_7.1a {
dimensions:
   lat = 3;
   lon = 3;
   nv = 2; // number of vertices

variables:
   float lat(lat);
	     lat:long_name = "latitude";
	     lat:units = "degrees_north";
	     lat:bounds = "lat_bnds";

   float lat_bnds(lat,lon,nv);
      lat_bnds:comment = "bounds of a coordinate variable: invalid number of dimensions" ;

// global attributes:
   :comment="Cells on a latitude axis" ;
   :Conventions = "CF-1.4" ;

data:
lat = 1., 2., 3.;
}