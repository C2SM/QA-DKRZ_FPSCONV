netcdf cf_7.3.3c {
dimensions:
   lat=2;
   lon=3;
   maxlen=20;
   ls=2;

variables:
   char land_sea(ls,maxlen);
      land_sea:standard_name="area_type";
   float lat(lat) ;
      lat:units = "degrees_north" ;
      lat:standard_name = "latitude" ;
      lat:axis = "Y" ;
   float lon(lon) ;
      lon:units = "degrees_east" ;
      lon:standard_name = "longitude" ;
      lon:axis = "X" ;

   float surface_t(ls,lat,lon);
      surface_t:long_name = "surface temperature" ;
      surface_t:coordinates="land_sea";
      surface_t:cell_methods="area: mean where not_land";
      surface_t:comment = "invalid type=not_land" ;

// global attributes:
   :comment="Mean surface temperature over land and sensible heat flux averaged separately over land and sea." ;
   :Conventions = "CF-1.4" ;

data:
 land_sea="land","sea";
 lat=1.,2.;
 lon=1.,2.,3.;
}