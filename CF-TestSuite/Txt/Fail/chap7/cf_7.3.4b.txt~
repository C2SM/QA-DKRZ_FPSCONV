netcdf cf_7.3.4b {
dimensions:
   time=3;
   nv=2;

variables:
   double time(time);
      time:standard_name="time" ;
      time:climatology="climatology_bounds";
      time:units="days since 1961-1-1";

   float t(time);
      t:long_name="surface air temperature";
      t:cell_methods="domain: mean time: mean";
      t:units="K";
      t:comment="cell_methods: not a supplied valid standard_name";

// global attributes:
   :comment="Cell-methods when there is no coordinate" ;
   :Conventions = "CF-1.4" ;

data:

  time= 1., 2., 3;
}
