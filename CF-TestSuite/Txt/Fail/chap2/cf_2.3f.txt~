netcdf cf_2.3f {
dimensions:
   lat = 1;
   Lat = 1;
   lon = 2;

variables:
   float PSL(lat,lon) ;
      PSL:long_name = "mean sea level pressure" ;
      PSL:units = "hPa" ;
      PSL:standard_name = "air_pressure_at_sea_level" ;
   float x(Lat,lon) ;
      x:long_name = "mean sea level pressure" ;
      x:units = "hPa" ;
      x:standard_name = "air_pressure_at_sea_level" ;

// global attributes:
   :comment="warning: same dimensions names when case is ignored" ;
   :Conventions = "CF-1.4" ;
}
