netcdf cf_2.5b {
dimensions:
   lat = 2;
   lon = 3;
   time = 2;
   lev=2;

variables:
   int ipsl(lat,lon) ;
      ipsl:units = "hPa" ;
      ipsl:standard_name = "air_pressure_at_sea_level" ;
      ipsl:_FillValue = 10.2f;
      ipsl:comment = "invalid: _FillValue and variable must be of the same type" ;

// global attributes:
   :comment="variables" ;
   :Conventions = "CF-1.4" ;
}
