netcdf cf_2.5 {
dimensions:
   lat = 2;
   lon = 3;
   time = 2;
   p=1;

variables:
   float lat(lat) ;
      lat:standard_name = "latitude" ;
      lat:units = "degrees_north" ;
   float lon(lon) ;
      lon:standard_name = "longitude" ;
      lon:units = "degrees_east" ;
   double time(time) ;
      time:long_name = "time" ;
      time:units = "days since 1990-1-1 0:0:0" ;
   float p(p) ;
      p:long_name = "pressure" ;
      p:standard_name = "air_pressure" ;
      p:units = "hPa" ;

   float psl_1(lat,lon,time,p) ;
      psl_1:long_name = "mean sea level pressure" ;
      psl_1:units = "hPa" ;
      psl_1:standard_name = "air_pressure_at_sea_level" ;
      psl_1:comment = "recommendation: order of dimensions should be T Z X Y" ;

// global attributes:
   :comment="variables" ;
   :Conventions = "CF-1.4" ;

data:
lat = 1., 2.;
lon = 1., 2., 3.;
time = 1., 2.;
p = 1.;
}
