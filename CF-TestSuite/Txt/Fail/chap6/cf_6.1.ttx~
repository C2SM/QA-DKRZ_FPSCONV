netcdf cf_6.1a {
dimensions:
   parcel = 2 ; // number of trajectories
   times = 3 ;
   max_len_parcel_name = 5 ; // max length of trajectory name

variables:
   float T(parcel,times) ;
      T:long_name = "temperature" ;
	     T:coordinates = "parcel_name lat lon" ;
      T:comment = "order of dimensions is valid, because times is not unlimited" ;
   float times(times) ;
      times:standard_name = "time" ;
      times:units="days since 1961-1-1";
   float lon(parcel,times) ;
      lon:standard_name = "longitude";
      lon:units= "degrees_east" ;
   float lat(parcel,times) ;
      lat:standard_name = "latitude";
      lat:units= "degrees_north" ;

   char parcel_name(max_len_parcel_name) ;
      parcel_name:comment="Note that ncgen would omit data strings qwer and yxcv" ;

// global attributes:
   :comment="Several parcel trajectories" ;
   :Conventions = "CF-1.4" ;

data:

times= 1., 2., 3. ;
parcel_name= "asdf", "qwer", "yxcv" ;
}

