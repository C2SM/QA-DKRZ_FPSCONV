netcdf cf_3.1 {
dimensions:
   lev = 2 ;
   lat = 2;
   lon = 3;

variables:
   float lat(lat);
      lat:long_name = "latitude";
      lat:units = "degrees_north";
   float lon(lon);
      lon:long_name = "latitude";
      lon:units = "degrees_east";
   float ps(lat,lon);
      ps:long_name = "pressure";
      ps:units = "hPa";

   float lev(lev) ;
    	 lev:long_name = "sigma at layer midpoints" ;
	     lev:positive = "down" ;
      lev:units= "level" ;
	     lev:standard_name = "atmosphere_sigma_coordinate" ;
	     lev:formula_terms = "sigma: lev ps: ps " ;
      lev:comment = "Warning: units=level is deprecated" ;

// global attributes:
   :comment="Atmosphere sigma coordinate" ;
   :Conventions = "CF-1.4" ;

data:
lat = 1., 2.;
lon = 1., 2.;
ps = 1., 2., 3., 4.;
}