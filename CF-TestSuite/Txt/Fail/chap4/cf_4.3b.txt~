netcdf cf_4.3b {
dimensions:
   h = 2 ;

variables:
   float h(h) ;
      h:long_name = "height" ;
      h:standard_name = "height" ;
      h:units= "m" ;
      h:comment = "invalid: missing attribute=positive" ;

// global attributes:
   :comment="Vertical coordinates" ;
   :Conventions = "CF-1.4" ;

data:
h = 1., 2. ;
}