netcdf cf_4.4d {
dimensions:
   time = UNLIMITED ; // (10 currently)

variables:
   double time(time) ;
    	 time:long_name = "time" ;
	     time:units = "days since 1990-1-1 0:0-0" ;
      time:comment = "invalid separator in the time part, must be colon" ;

// global attributes:
   :comment="Time axis" ;
   :Conventions = "CF-1.4" ;

data:
time = 1., 2.;
}