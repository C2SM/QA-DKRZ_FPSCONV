netcdf cf_4.3 {
dimensions:
   h1 = 2 ;
   h2 = 2 ;

variables:
   float height(h1) ;
      height:long_name = "height" ;
      height:standard_name = "height" ;
      height:positive= "up" ;
      height:axis= "z" ;
      height:comment = "invalid: missing attribute=units, axis not allowed" ;
   float h2(h2) ;
      h2:long_name = "height" ;
      h2:standard_name = "height" ;
      h2:units= "m" ;
      h2:comment = "invalid: missing attribute=positive" ;

// global attributes:
   :comment="Vertical coordinates" ;
   :Conventions = "CF-1.4" ;

data:
h2 = 1., 2. ;
}