netcdf cf_4.4e {
dimensions:
   time = UNLIMITED ; // (3 currently)

variables:
   double time(time) ;
    	 time:standard_name = "time" ;
      time:axis = "T" ;
      time:comment ="invalid: missing units" ;

// global attributes:
   :comment="Time axis" ;
   :Conventions = "CF-1.4" ;

data:

time = 0., 1., 2.;
}