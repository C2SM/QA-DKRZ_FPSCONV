netcdf cf_4.3c {
dimensions:
   height = 2 ;

variables:
   float height(height) ;
      height:standard_name = "height" ;
      height:comment = "invalid: coordinate variable must have units" ;

// global attributes:
   :comment="Vertical coordinates" ;
   :Conventions = "CF-1.4" ;

data:
height = 1., 2. ;
}