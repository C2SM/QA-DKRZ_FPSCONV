netcdf cf_4.1 {
dimensions:
   lat1 = 3 ;
   lat2 = 3 ;

variables:
   float lat1(lat1) ;
    	 lat1:long_name = "latitude" ;
	     lat1:standard_name = "latitude" ;
      lat1:units = "degrees_north" ;
   float lat2(lat2) ;
      lat2:long_name = "latitude" ;
      lat2:standard_name = "latitude" ;
      lat2:units = "degrees_north" ;
      lat2:comment = "invalid: only one coordinate variable per axis" ;

// global attributes:
   :comment="Latitude axis" ;
   :Conventions = "CF-1.4" ;

data:
lat1 = 1., 2., 3.;
lat2 = 1., 2., 3.;
}