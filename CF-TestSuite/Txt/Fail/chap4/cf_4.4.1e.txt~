netcdf cf_4.4.1e {
dimensions:
   time = UNLIMITED ; // (3 currently)

variables:
   double time(time) ;
    	 time:long_name = "time" ;
	     time:units = "days since 1-1-1 0:0:0" ;
	     time:calendar = "126 kyr B.P." ;
      time:leap_year = 1900 ;
	     time:leap_month = 11 ;
      time:comment ="invalid: missing month-lengths vector" ;

// global attributes:
   :comment="Paleoclimate time axis" ;
   :Conventions = "CF-1.4" ;

data:
time = 0., 1., 2. ;
}