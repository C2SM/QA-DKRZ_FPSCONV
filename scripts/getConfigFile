export CONFIG_FILE

if [ ! ${CONFIG_FILE} ] ; then
   export CONFIG_FILE=${HOME}/.qa-dkrz/config.txt
fi

gCF_retVal=0

if [ ! -f ${CONFIG_FILE} ] ; then
  mkdir -p ${CONFIG_FILE%/*} 2> /dev/null
  touch ${CONFIG_FILE} 2> /dev/null
  gCF_retVal=1
fi

test ! -s ${CONFIG_FILE} && gCF_retVal=1

return $gCF_retVal
